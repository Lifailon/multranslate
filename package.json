{
  "name": "multranslate",
  "version": "0.1.0",
  "description": "Cross-platform TUI for translating text using multiple providers simultaneously and support automatic detection of the source and destination language",
  "keywords": [
    "TUI",
    "Blessed",
    "Translate",
    "Google-Translate",
    "DeepL",
    "DeepLX",
    "mymemory",
    "Reverso"
  ],
  "author": "Lifailon",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Lifailon/multranslate"
  },
  "main": "multranslate.js",
  "scripts": {
    "start": "multranslate.js"
  },
  "dependencies": {
    "axios": "^1.7.5",
    "blessed": "^0.1.81"
  },
  "bin": {
    "multranslate": "multranslate.js"
  },
  "pkg": {
    "assets": [
      "node_modules/axios/**/*",
      "node_modules/blessed/**/*"
    ]
  }
}