{
    "version": "0.3.0",
    "configurations": [
        {
            "name": "Start all translators",
            "type": "node",
            "request": "launch",
            "runtimeExecutable": "npm",
            "runtimeArgs": [
                "start"
            ],
            "console": "integratedTerminal"
        },
        {
            "name": "Google",
            "type": "node",
            "request": "launch",
            "program": "${workspaceFolder}/multranslate-custom-cursor.js",
            "args": [
                "--translator",
                "google"
            ],
            "console": "integratedTerminal"
        },
        {
            "name": "DeepL",
